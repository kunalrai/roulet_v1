
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1 class="page-title">
    Winners
</h1>

<div class="page-bar">
    <ul class="page-breadcrumb">
        <li>
            <i class="icon-trophy"></i>
            <a href="/logs/index/">Logs</a>
        </li>
    </ul>
    <div class="page-toolbar select-date">
        <div class="pull-right form-group">
            <input type="text" id="date" />
        </div>
    </div>
</div>

<!-- END PAGE HEADER-->

<div class="row">

    <div class="col-md-12">
        <div class="portlet light portlet-fit portlet-datatable ">
            <div class="portlet-title">
                <div class="caption">
                    <i class="icon-settings font-green"></i>
                    <span class="caption-subject font-green sbold uppercase"> Users </span>
                </div>
            </div>
            <div class="portlet-body">
                <div class="table-container">
                    <table class="table table-striped table-bordered table-hover table-checkable" id="logs-table">
                        <thead>
                            <tr role="row" class="heading">
                                <th width="5%"> Name </th>
                                <th width="15%"> Type </th>
                                <th width="15%"> Coin</th>
                                <th width="10%" class="sorting_disabled"> Date </th>
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>
    </div>

    <!-- AJAX MODAL -->

    <div class="modal fade in" id="ajax">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-body">
                    <img src"~/www/assets/global/img/loading-spinner-grey.gif" alt="" class="loading"/>
                    <span> &nbsp;&nbsp;Saving.. </span>
                </div>
            </div>
        </div>
    </div>

    <!-- AJAX MODAL -->

</div>





